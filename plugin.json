{
    "description": "Test Plugin",
    "command": "bash index.sh $(name) $(poll) $(data)",
    "metrics" : [ "TEST_METRIC"],
    "paramSchema": [
        {
            "title": "Name",
            "name": "name",
            "description": "Name of this metric configuration",
            "type": "string",
            "default": "",
            "required": false
        },
        {
            "title": "Poll",
            "name": "poll",
            "description": "The Poll Interval to call the command. Defaults 5 seconds",
            "type": "string",
            "default": 5,
            "required": false
        },
        {
            "title": "Data",
            "name": "data",
            "description": "Data to pass to the script",
            "type": "string",
            "default": "",
            "required": false
        }
    ]
}

